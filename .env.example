
# ========== API Keys ==========
# DeepSeek API Key - 从 https://platform.deepseek.com/ 获取
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# 智谱AI API Key - 从 https://open.bigmodel.cn/ 获取
ZHIPU_API_KEY=your_zhipu_api_key_here

# ========== Neo4j配置 ==========
# Neo4j 数据库连接地址
NEO4J_URI=neo4j://0.0.0.0:7687

# Neo4j 用户名
NEO4J_USER=neo4j

# Neo4j 密码
NEO4J_PASSWORD=your_neo4j_password_here

# Neo4j 是否使用加密连接
NEO4J_ENCRYPTED=False

# ========== Redis配置 ==========
# Redis 主机地址
REDIS_HOST=0.0.0.0

# Redis 端口
REDIS_PORT=6379

# Redis 数据库编号
REDIS_DB=0

# Redis 密码（如果设置了密码）
REDIS_PASSWORD=

# Redis 最大连接数
REDIS_MAX_CONNECTIONS=10

# ========== 服务端口配置 ==========
# Agent 服务端口
AGENT_SERVICE_PORT=8103

# Graph 服务端口
GRAPH_SERVICE_PORT=8101

# Red Spider 服务端口（如果使用）
RED_SPIDER_SERVICE_PORT=5001
